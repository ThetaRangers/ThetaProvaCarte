<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="it.thetarangers.provacarte" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApp.NoActionBar" >
        <activity android:name="it.thetarangers.provacarte.SwipeCard" >
        </activity>
        <activity
            android:name="it.thetarangers.provacarte.ListActivity" />
        <activity android:name="it.thetarangers.provacarte.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="it.thetarangers.provacarte.DragActivity" />
        <activity android:name="it.thetarangers.provacarte.EventsActivity" />
        <activity android:name="it.thetarangers.provacarte.EventDetailActivity" />
    </application>

</manifest>